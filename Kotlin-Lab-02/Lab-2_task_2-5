import java.io.File
fun main() {
    val input = File("input.txt")
    val output = File("output.txt")
    var num = input.readLines()[1]
      .split(Regex(" "))
        .map { it.toInt() }
    var s: String = ""
    var list = num.toMutableList()
    var index = (input.readLines()[2].toDouble().toInt())
    var a:Int = list.get(index)
    list.removeAt(index)
    list.add(0,a)

    list.forEach {
        s += it.toString()
    }
    output.writeText("$s")
}
