import java.io.File
import kotlin.math.absoluteValue

val lines = File("input.txt").readLines()

fun main() {

    var answer = ""

    lines[0].split(" ")
        .map { it.toInt().absoluteValue }
        .sortedDescending()
        .forEach { answer += "$it " }

    printToFile(answer)
}

fun printToFile(string: String) {
    File("output.txt").writeText(string)
    println(string)
}
