import java.io.File

val lines = File("input.txt").readLines()

fun main() {

    var answer = ""

    lines[0].split(" ")
        .map { it.toInt() }
        .sortedDescending()
        .forEach { answer += "$it " }

    printToFile(answer)
}

fun printToFile(string: String) {
    File("output.txt").writeText(string)
}
