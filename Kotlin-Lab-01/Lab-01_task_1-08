import java.io.File

val lines = File("input.txt").readLines()

fun main() {

    var answer = mutableListOf<Int>()

    lines[0].split(" ")
        .map { it.toInt() }
        .sortedDescending()
        .forEach { answer.add(it) }

    printToFile(answer)
}

fun printToFile(answers: List<Int>) {
    var string = ""

    for (i in 0..answers.lastIndex) {
        string += answers[i]

        if (i != answers.lastIndex)
            string += " "
    }

    printToFile(string)
}

fun printToFile(string: String) {
    File("output.txt").writeText(string)
    println(string)
}
